#: templates/ali/search.html:122
msgid "Minimum price"
msgstr "Preço mínimo"

#: templates/ali/search.html:128
msgid "Mean price"
msgstr "Preço mediano"

#: templates/ali/search.html:134
msgid "Average price"
msgstr "Preço médio"

#: templates/ali/search.html:140
